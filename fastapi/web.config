<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <system.webServer>
    <handlers>
      <add name="PythonHandler" path="*" verb="*" modules="httpPlatformHandler" resourceType="Unspecified"/>
    </handlers>
    <httpPlatform processPath="D:\python_env\python.exe"
                  arguments="-m uvicorn --port %HTTP_PLATFORM_PORT% fastapi-main:app"
                  stdoutLogEnabled="false"
                  stdoutLogFile="D:\MyWebAPI\Log.txt"
                  startupTimeLimit="60"
                  processesPerApplication="16">
    </httpPlatform>
  </system.webServer>
</configuration>